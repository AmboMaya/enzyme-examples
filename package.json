{
  "name": "enzyme-examples",
  "version": "0.2.0",
  "description": "Simple Enzyme testing of React components",
  "repository": "https://github.com/dev-academy-challenges/enzyme-examples",
  "scripts": {
    "start": "webpack-dev-server --content-base public/ --hot --inline",
    "watch": "nodemon -i test/bundle.js -e js,jsx -x npm test",
    "test": "ava test/*.test.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/setup-env.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "jsdom": "^10.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.0.0-pre.5",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^1.16.2"
  }
}
